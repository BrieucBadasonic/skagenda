<div class="container">
  <% @events.each do |event| %>
    <div class="card-product">
      <%= cl_image_tag event.photo.key, height: 400, width: 300  %>
      <div class="card-product-infos">
        <% event.bands.each do |band| %>
         <h2><%= band.name %> </h2>
        <% end %>
        <p><%= event.venue.name %> - <%= event.venue.address %></p>
        <p><%= event.date %> - <%= event.price %> Euros</p>
      </div>
    </div>
  <% end %>
</div>





